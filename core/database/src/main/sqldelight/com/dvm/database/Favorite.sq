CREATE TABLE IF NOT EXISTS favorite(
    dishId TEXT NOT NULL PRIMARY KEY
);


isFavorite:
SELECT (
    EXISTS(
        SELECT 1
        FROM favorite
        WHERE favorite.dishId = :dishId
    )
);


favorites:
SELECT dishId
FROM favorite;


insert:
INSERT OR IGNORE INTO favorite(
    dishId
)
VALUES (?);


delete:
DELETE FROM favorite
WHERE dishId = :dishId;


deleteAll:
DELETE FROM favorite;