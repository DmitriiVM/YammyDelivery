import java.util.Date;

CREATE TABLE IF NOT EXISTS hint(
    query TEXT NOT NULL PRIMARY KEY,
    date INTEGER AS Date NOT NULL
);


hints:
SELECT query
FROM hint
ORDER BY date DESC;


hintCount:
SELECT COUNT()
FROM hint;


delete:
DELETE
FROM hint
WHERE query = :hint;


deleteOldest:
DELETE
FROM hint
WHERE date = (
    SELECT MIN(date)
    FROM hint
);


insert:
INSERT OR REPLACE INTO hint(
    query,
    date
)
VALUES ?;